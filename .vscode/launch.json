{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 5173,
            "urlFilter": "http://localhost:5173/signin",
            "webRoot": "${workspaceFolder}"
            //notworking
        },
        // configuration for Google Chrome normal and incognito
        {
            "name": "Google Chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173/signin",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ]
            //working
        },
        {
            "name": "Google Chrome Ingognito",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:5173/signin",
            "webRoot": "${workspaceRoot}",
            "sourceMaps": true,
            "runtimeArgs": [
                "--remote-debugging-port=9222",
                "-incognito"
            ]
            //working
        },
        {
            "name": "Google Chrome Normal Non-Headless",
            "type": "chrome",
            "request": "launch",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:5173/signin",
            //working
        },
        {
            "name": "Google Chrome Headless",
            "type": "chrome",
            "request": "launch",
            "runtimeArgs": [
                "--headless",
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:5173/signin",
            //notworking
        },
        // for Microsoft edge
        {
            "name": "Edge",
            "type": "msedge",
            "request": "launch",
            "url": "http://localhost:5173/",
            "webRoot": "${workspaceFolder}",
            "sourceMaps": true,
            "trace": true,
            "userDataDir": "${workspaceRoot}/.vscode/edge"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Launch Edge against localhost",
            "url": "http://localhost:5173",
            "sourceMaps": true
            // "webRoot": "${workspaceFolder}"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Open index.html",
        },
        {
            "name": "Attach to Edge",
            "port": 9222,
            "request": "attach",
            "type": "msedge",
            "webRoot": "${workspaceFolder}"
        },
        {
            "type": "pwa-msedge",
            "name": "Launch Microsoft Edge",
            "request": "launch",
            "runtimeArgs": [
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:5173/signin",
            "presentation": {
                "hidden": true
            }
        },
        {
            "type": "pwa-msedge",
            "name": "Launch Microsoft Edge in headless mode",
            "request": "launch",
            "runtimeArgs": [
                "--headless",
                "--remote-debugging-port=9222"
            ],
            "url": "http://localhost:5173/signin",
            "presentation": {
                "hidden": true
            }
        },
        {
            "type": "vscode-edge-devtools.debug",
            "name": "Open Edge DevTools",
            "request": "attach",
            // "url": "file://d:\\block\\project\\consoleTest\\index.html",
            "presentation": {
                "hidden": true
            }
        }
    ],
    "compounds": [
        {
            "name": "Launch Edge Headless and attach DevTools",
            "configurations": [
                "Launch Microsoft Edge in headless mode",
                "Open Edge DevTools"
            ]
        },
        {
            "name": "Launch Edge and attach DevTools",
            "configurations": [
                "Launch Microsoft Edge",
                "Open Edge DevTools"
            ]
        },
        // {
        //     "name": "L",
        //     "configurations": [
        //         "Chrome Chrome Chrome",
        //     ]
        // },
    ]
}